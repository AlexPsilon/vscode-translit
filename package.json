{
    "name": "translit",
    "displayName": "Translit",
    "description": "Transliteration cyrillic to latin",
    "version": "1.0.0",
    "publisher": "Psilon",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.translitCyrToLat"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.translitCyrToLat",
            "title": "Translit cyr -> lat"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
